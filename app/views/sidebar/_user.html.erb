<div class="span3">
  <a rel="tooltip" title="Share your work with others" data-placement="bottom" style="margin-bottom:6px;" href="/post" class="btn btn-primary btn-large btn-block"><i class="icon-pencil icon-white"></i> Post research &raquo;</a>
  <a rel="tooltip" title="Collaborative pages others can edit" data-placement="bottom" href="/wiki/new" class="btn btn-block"><i class="icon-book"></i> Create wiki page &raquo;</a>
  <hr />

  <iframe frameborder="0" height="380" src="http://www.kickstarter.com/projects/publiclab/infragram-the-infrared-photography-project/widget/card.html" width="220"></iframe>

  <% if @wikis && @wikis.length > 0 %>
  <div class="hidden-phone">
  <h4>Related wiki pages</h4>
  <ul class="nav wikis bullet">
    <% @wikis.each do |wiki| %>
      <li><a href="<%= wiki.path %>"><i class="icon-book"></i> <%= wiki.title %></a> <small style="color:#888;">(updated <%= time_ago_in_words(wiki.updated_at) %> ago)</small></li>
    <% end %>
  </ul>
  <% end %>
  </div>

</div>
