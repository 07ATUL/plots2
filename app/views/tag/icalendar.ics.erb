BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//publiclab/events//NONSGML v1.0//EN
<% @notes.each do |node| %>
BEGIN:VEVENT
UID:do-not-reply@publiclab.org
<% if node.power_tag('date') != '' %>
<% begin %>
DTSTAMP:<%= DateTime.parse(node.power_tag('date')).strftime("%Y%m%dT%H%M%S") %>
DTSTART;VALUE=DATE:<%= DateTime.parse(node.power_tag('date')).strftime("%Y%m%d") %>
DTEND;VALUE=DATE:<%= DateTime.parse(node.power_tag('date')).strftime("%Y%m%d")[0..-2] + (DateTime.parse(node.power_tag('date')).strftime("%Y%m%d")[-1].to_i + 1).to_s %>
<% rescue %>
<% end %>
<% end %>
SUMMARY:<%= node.title %>
END:VEVENT
<% end %>
END:VCALENDAR
